{
  "name": "@srothgan/sanity-plugin-autocomplete-input",
  "version": "3.1.0",
  "description": "Autocomplete input plugin for Sanity",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "autocomplete",
    "input",
    "v4",
    "v5",
    "maintained",
    "sanity-v5"
  ],
  "license": "MIT",
  "author": "Simon Rothgang <simonrothgang@gmail.com>",
  "contributors": [
    {
      "name": "Liam Martens",
      "email": "liam@freighter.studio",
      "url": "https://github.com/LiamMartens"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "format:check": "prettier --check --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/srothgan/srothgan-sanity-plugin-autocomplete-input"
  },
  "bugs": {
    "url": "https://github.com/srothgan/srothgan-sanity-plugin-autocomplete-input/issues"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "just-compact": "^3.2.0",
    "just-pick": "^4.2.0",
    "just-safe-get": "^4.2.0",
    "just-unique": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.39.2",
    "@sanity/pkg-utils": "^10.2.3",
    "@sanity/plugin-kit": "^4.0.20",
    "@sanity/ui": "^3.1.11",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sanity": "^5.1.0",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@sanity/ui": "^2 || ^3",
    "react": "^18 || ^19",
    "sanity": "^4 || ^5"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  }
}
