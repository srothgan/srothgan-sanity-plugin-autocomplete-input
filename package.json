{
  "name": "@srothgan/sanity-plugin-autocomplete-input",
  "version": "3.0.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:js": "swc ./src -d ./lib --strip-leading-paths",
    "build:types": "tsc -p .",
    "build": "run-p build:*",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\""
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/srothgan/srothgan-sanity-plugin-autocomplete-input"
  },
  "bugs": {
    "url": "https://github.com/srothgan/srothgan-sanity-plugin-autocomplete-input/issues"
  },
  "author": {
    "name": "Simon Rothgang",
    "email": "simonrothgang@gmail.com"
  },
  "contributors": [
    {
      "name": "Liam Martens",
      "email": "liam@freighter.studio",
      "url": "https://github.com/LiamMartens"
    }
  ],
  "keywords": [
    "sanity-plugin",
    "autocomplete",
    "v4",
    "v5",
    "maintained",
    "input",
    "sanity-v5"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sanity/ui": "^3.1.11",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.3.99",
    "@types/node": "^20.19.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sanity": "^5.1.0",
    "styled-components": "^6.1.19",
    "typescript": "~5.3.2"
  },
  "dependencies": {
    "just-compact": "^3.2.0",
    "just-pick": "^4.2.0",
    "just-safe-get": "^4.2.0",
    "just-unique": "^4.2.0"
  }
}
